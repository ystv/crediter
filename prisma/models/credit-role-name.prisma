model CreditRoleName {
  id                       String  @id @default(cuid())
  name  String

  order Int

  credit_role_id String
  credit_role CreditRole @relation(fields: [credit_role_id], references: [id], onDelete: Cascade)

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt()

  @@map("credit_role_name")
}